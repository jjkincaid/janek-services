          <div class="mb-3">
            <label for="apartment-number" class="form-label">Apartment #</label>
            <input type="address" class="form-control" placeholder="Apartment #">
          </div>
          <div class="mb-3">
            <label for="phone-number" class="form-label">Phone - Daytime</label>
            <input type="phone" class="form-control" placeholder="Phone">
          </div>
          <div class="mb-3">
            <label for="phone-number" class="form-label">Phone - Evening</label>
            <input type="phone" class="form-control" placeholder="Phone">
          </div>
          <div class="mb-3">
            <label for="phone-number" class="form-label">Cell Phone</label>
            <input type="phone" class="form-control" placeholder="Phone">
