              <div class="mb-3">
                <label for="other" class="form-label"></label>
                <textarea class="form-control" rows="5" placeholder="Position Duties"></textarea>
              </div>
              <div class="mb-3">
                <label for="other" class="form-label"></label>
                <textarea class="form-control" rows="5" placeholder="Reason for Leaving"></textarea>
              </div>

            <h5>Employer 2</h5>
             <div class="mb-3">
                <label for="employment" class="form-label"></label>
                <input type="learn" class="form-control" placeholder="Position Title">
             </div>
              <div class="mb-3">
                <input type="learn" class="form-control" placeholder="Start Date">
              </div>
             <div class="mb-3">
                <input type="learn" class="form-control" placeholder="End Date">
